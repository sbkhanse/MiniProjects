CREATE TABLE GUEST_TAB (
		GSTID NUMBER(4) PRIMARY KEY, 
		GSTNAME VARCHAR2(40), 
		EMAIL VARCHAR2(30), 
		MOBNO NUMBER(10),
		USERNAME VARCHAR2(25), 
		PASSWORD VARCHAR2(15)
	);
	
CREATE SEQUENCE GSTID_SEQ START WITH 100 INCREMENT BY 1;	


CREATE TABLE ROOM_TAB(
		ROOM_ID NUMBER(3) PRIMARY KEY,
		ROOM_TYPE VARCHAR2(20),
		STATUS VARCHAR2(10),
		START_DATE DATE,
		END_DATE DATE
	);